{
  "name": "bibfetch",
  "displayName": "bibfetch",
  "description": "Fetch bibtex information from academic search engines like dblp.",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.75.0"
  },
  "license": "GPL-3.0",
  "repository": {
		"type": "git",
		"url": "https://github.com/yzh119/bibfetch"
	},
  "publisher": "expye",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:bibfetch.searchBibtex"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "bibfetch.searchBibtexDefault",
        "title": "bibfetch: search bibtex of an article with default setting."
      },
      {
        "command": "bibfetch.searchBibtexWith",
        "title": "bibfetch: search bibtex of an article with custom setting."
      },
      {
        "command": "bibfetch.searchBibtexForSelectedText",
        "title": "Search bibtex of selected text"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "bibfetch.searchBibtexForSelectedText",
          "when": "editorTextFocus"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4",
    "@vscode/test-electron": "^2.2.2"
  }
}
